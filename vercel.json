{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!canva-callback\\.html|login\\.html|signup\\.html|index\\.html|assets/).*)",
      "destination": "/index.html"
    }
  ]
}

